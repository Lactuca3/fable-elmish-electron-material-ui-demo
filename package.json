{
  "name": "fable-elmish-electron-demo",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "electronWebpack": {
    "title": true,
    "main": {
      "sourceDirectory": "src/Main",
      "webpackConfig": "webpack.additions.main.js"
    },
    "renderer": {
      "sourceDirectory": "src/Renderer",
      "webpackConfig": "webpack.additions.renderer.js"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.5",
    "@material-ui/icons": "^4.4",
    "@material-ui/lab": "4.0.0-alpha.28",
    "autosuggest-highlight": "^3.1",
    "downshift": "^3.3",
    "match-sorter": "^4.0",
    "mdi-material-ui": "^6.7",
    "react": "^16.10",
    "react-dom": "^16.10",
    "source-map-support": "^0.5",
    "toastr": "^2",
    "typeface-roboto": "^0"
  },
  "devDependencies": {
    "@babel/core": "^7.6",
    "bufferutil": "^4.0",
    "devtron": "^1.4",
    "electron": "^6.0",
    "electron-builder": "^21.1",
    "electron-devtools-installer": "^2.2",
    "electron-webpack": "^2.6",
    "electron-window-state": "^5.0",
    "fable-compiler": "^2.4",
    "fable-loader": "^2.1",
    "node-sass": "^4.12",
    "remotedev": "^0.2",
    "sass-loader": "^8.0",
    "utf-8-validate": "^5.0",
    "webpack": "^4.41"
  }
